{
  "manifest_version": 2,

  "name": "Duke2Dook",
  "version": "1.1.3",
  "description": "Turns every mention of Duke on any website into 'Dook'",
  "permissions": ["activeTab"],
  "icons": {
    "128": "images/dukelogo.png"
  },
  "content_scripts": [
    {
    "matches": ["http://*/*","https://*/*"],
    "js": ["content.js"],
    "run_at":"document_end"
    }
  ]

}
